                        <div class="form-group">
                          <label for="serviceBy">Service By</label>
                          <input type="text" class="form-control" name="service_by" placeholder="Your Name here"
                            id="serviceBy" required />
                        </div>
                        <div class="form-group">
                          <label for="serviceType">Service Type</label>
                          <select id="serviceType" class="form-control" name="service_type" required>
                            <option value="">-- Select --</option>
                            <option value="tiffin">Tiffin</option>
                            <option value="cab">Cab</option>
                            <option value="housekeep">House Keeping</option>
                          </select>
                        </div>
                        <div class="form-group">
                          <label for="serviceDesc">Service Description</label>
                          <textarea class="form-control" name="service_desc" placeholder="Service Description Here"
